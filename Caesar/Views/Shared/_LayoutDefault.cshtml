<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    @Html.Raw(ViewBag.Title)
    @Html.Raw(ViewBag.dcTitle)
    @Html.Raw(ViewBag.Description)
    @Html.Raw(ViewBag.dcDescription)
    @Html.Raw(ViewBag.Keyword)
    @Html.Raw(ViewBag.canonical)
    @Html.Raw(ViewBag.Meta)
    <meta name="geo.region" content="VN-HN" />
    <meta name="geo.placename" content="H&agrave; Nội" />
    <meta name="geo.position" content="20.981367;105.811074" />
    <meta name="ICBM" content="20.981367, 105.811074" />
    <meta http-equiv="content-language" content="vi" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/Content/Display/Css/Default.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Default_Res.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/News.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/News_Res.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/nivo-slider.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Order.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Order_res.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Product.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Product_Res.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/style.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/jquery.mmenu.all.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/demo.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <link href="~/Content/Display/Css/Slide.css" rel="stylesheet" />
    <script src="~/Scripts/GoogleAnalytics.js"></script>
     
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Content/Display/Script/jquery-1.9.0.min.js"></script>
</head>
<body>
    @{Html.RenderAction("PartialTop", "Default");}
    <div class="Clear"></div>
    @{Html.RenderAction("PartialBanner", "Default");}
    <div class="Clear"></div>@{Html.RenderAction("PartialMenuMobile", "Default");}
    @{Html.RenderAction("PartialMenu", "Default");} <div id="Content">

        @RenderBody()
        <div class="Clear"></div>
    </div>
    <div class="Clear"></div>
    <div id="Services">
        <div class="sv0"></div>
        <div class="sv1">  </div>
        <div class="sv2"></div>
        <div class="sv3"></div>
        <div class="sv4"></div>
        <div class="Clear"></div>
    </div>
    <div class="Clear"></div>
    <div id="Footer">
        @{Html.RenderAction("PartialFootter", "Default");}
        <div class="Clear"></div>
        <div id="Bottom">
            <div id="Contentss">
                <span>
                    Copyright 2015  SEABIG VIET NAM . Allright reserved
                </span>
                <span id="connectedUsers"></span>

                <script type="text/javascript">
                    function PollUsers() {
                        $(function () {
                            $.getJSON("/Default/UserConnected", function (json) { $('#connectedUsers').text("- Online on Site : " + json.count + " user(s) connected") });
                        });
                    }

                    setInterval(PollUsers, 30000);
                </script>
                <div id="Right">
                    <a href="" class="Face"></a>
                    <a href="" class="Google"></a>
                    <a href="" class="Youtube"></a>
                </div>
            </div>

        </div>
        <div class="Clear"></div>
       
        <script src="~/Content/Display/Script/jquery.nivo.slider.js"></script>
        <script src="~/Content/Display/Script/jquery.nivo.slider.pack.js"></script>
        <script src="~/Scripts/plusone.js"></script>
        <script type="text/javascript">
            $(window).load(function () {
                $('#slider').nivoSlider();
            });
        </script>
        <script>
            window.fbAsyncInit = function () {
                FB.init({
                    appId: '1600906160151458',
                    xfbml: true,
                    version: 'v2.3'
                });
            };

            (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0];
                if (d.getElementById(id)) { return; }
                js = d.createElement(s); js.id = id;
                js.src = "//connect.facebook.net/en_US/sdk.js";
                fjs.parentNode.insertBefore(js, fjs);
            }(document, 'script', 'facebook-jssdk'));
        </script>
        <script src="~/Scripts/jquery.mmenu.min.all.js"></script>
        <script type="text/javascript">
            $(function () {
                $('nav#menu').mmenu({
                    extensions: ['effect-slide', 'pageshadow'],
                    header: true,
                    searchfield: true,
                    counters: true,
                    footer: {
                        add: true,

                    }
                });
            });
        </script>
    </div>
</body>
</html>
